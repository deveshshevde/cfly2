<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:gravity="center" android:id="@android:id/background" android:height="2dp">
        <shape>
            <corners android:radius="5dp"/>
            <solid android:color="@color/white"/>
        </shape>
    </item>
    <item android:gravity="center" android:id="@android:id/secondaryProgress" android:height="2dp">
        <clip>
            <shape>
                <corners android:radius="5dp"/>
                <solid android:color="@color/color_999999"/>
            </shape>
        </clip>
    </item>
    <item android:gravity="center" android:id="@android:id/progress" android:height="2dp">
        <clip>
            <shape>
                <corners android:radius="5dp"/>
                <gradient android:startColor="#2896f0" android:endColor="#2896f0" android:angle="0"/>
            </shape>
        </clip>
    </item>
</layer-list>
