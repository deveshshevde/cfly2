<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#172d49"/>
            <stroke android:width="1dp" android:color="#6d96a7"/>
            <corners android:radius="5dp"/>
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <solid android:color="#60b39e"/>
            <stroke android:width="1dp" android:color="#699c91"/>
            <corners android:radius="5dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#2f688b"/>
            <stroke android:width="1dp" android:color="#4982a6"/>
            <corners android:radius="5dp"/>
        </shape>
    </item>
</selector>
