<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#ca258e"/>
            <stroke android:width="1dp" android:color="#a34c83"/>
            <corners android:radius="5dp"/>
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <solid android:color="#b357a0"/>
            <stroke android:width="1dp" android:color="#c062c7"/>
            <corners android:radius="5dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#b14040"/>
            <stroke android:width="1dp" android:color="#ac3e3e"/>
            <corners android:radius="5dp"/>
        </shape>
    </item>
</selector>
