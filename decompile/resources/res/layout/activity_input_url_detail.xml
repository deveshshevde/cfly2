<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:id="@+id/activity_detail_player" android:layout_width="match_parent" android:layout_height="match_parent">
    <androidx.core.widget.NestedScrollView android:id="@+id/post_detail_nested_scroll" android:layout_width="match_parent" android:layout_height="wrap_content" android:fillViewport="true">
        <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content">
            <View android:layout_width="match_parent" android:layout_height="200dp"/>
            <TextView android:textSize="16sp" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="详情"/>
        </LinearLayout>
    </androidx.core.widget.NestedScrollView>
    <com.xeagle.android.login.gsy.video.LandLayoutVideo android:id="@+id/detail_player" android:layout_width="match_parent" android:layout_height="200dp"/>
    <ImageView android:id="@+id/test_image_view" android:layout_width="200dp" android:layout_height="200dp" android:layout_margin="10dp" android:src="@drawable/ic_launcher" android:layout_alignParentRight="true" android:layout_alignParentBottom="true"/>
    <Button android:id="@+id/inputUrl" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="10dp" android:text="切换url" android:layout_alignParentRight="true" android:layout_alignParentBottom="true"/>
</RelativeLayout>
