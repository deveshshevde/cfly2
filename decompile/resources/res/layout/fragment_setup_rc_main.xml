<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:gravity="center" android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="match_parent" style="@style/configLayout">
    <RelativeLayout android:gravity="center" android:id="@+id/relativeLayout2" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_margin="3dp" android:layout_weight="1">
        <org.droidplanner.android.widgets.FillBar.FillBar android:id="@+id/fillBar_throttle" android:layout_width="@dimen/fillbar_thickness" android:layout_height="@dimen/fillbar_length" android:layout_marginRight="20dp" android:layout_toLeftOf="@+id/stickLeft" android:layout_alignTop="@+id/stickLeft" android:layout_alignBottom="@+id/stickLeft"/>
        <org.droidplanner.android.widgets.RcStick.RcStick android:id="@+id/stickLeft" android:layout_width="@dimen/setup_rc_stick_width" android:layout_height="@dimen/setup_rc_stick_height" android:layout_alignParentRight="true"/>
        <TextView android:id="@+id/thr_yaw_text" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="8dp" android:text="Throttle: 1000\10Yaw: 1500" android:layout_below="@+id/fillBar_yaw" android:layout_alignLeft="@+id/fillBar_yaw" style="@style/rc_textTitle"/>
        <org.droidplanner.android.widgets.FillBar.FillBar android:id="@+id/fillBar_yaw" android:layout_width="@dimen/fillbar_length" android:layout_height="@dimen/fillbar_thickness" android:layout_marginTop="25dp" android:layout_below="@+id/stickLeft" android:layout_alignLeft="@+id/stickLeft" android:layout_alignRight="@+id/stickLeft"/>
    </RelativeLayout>
    <RelativeLayout android:gravity="center" android:id="@+id/relativeLayout3" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_margin="3dp" android:layout_weight="1">
        <org.droidplanner.android.widgets.FillBar.FillBar android:id="@+id/fillBar_pitch" android:layout_width="@dimen/fillbar_thickness" android:layout_height="@dimen/fillbar_length" android:layout_marginLeft="20dp" android:layout_toRightOf="@+id/stickRight" android:layout_alignTop="@+id/stickRight" android:layout_alignBottom="@+id/stickRight"/>
        <org.droidplanner.android.widgets.RcStick.RcStick android:id="@+id/stickRight" android:layout_width="@dimen/setup_rc_stick_width" android:layout_height="@dimen/setup_rc_stick_height" android:layout_alignParentLeft="true"/>
        <TextView android:id="@+id/roll_pitch_text" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="8dp" android:text="Roll: 1500\10Pitch: 1500" android:layout_below="@+id/fillBar_roll" android:layout_alignLeft="@+id/fillBar_roll" style="@style/rc_textTitle"/>
        <org.droidplanner.android.widgets.FillBar.FillBar android:id="@+id/fillBar_roll" android:layout_width="@dimen/fillbar_length" android:layout_height="@dimen/fillbar_thickness" android:layout_marginTop="25dp" android:layout_below="@+id/stickRight" android:layout_alignLeft="@+id/stickRight" android:layout_alignRight="@+id/stickRight"/>
    </RelativeLayout>
    <LinearLayout android:gravity="center" android:orientation="vertical" android:id="@+id/linearLayout1" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_margin="3dp" android:layout_weight="1">
        <org.droidplanner.android.widgets.FillBar.FillBar android:id="@+id/fillBar_ch_5" android:layout_width="@dimen/fillbar_length" android:layout_height="@dimen/fillbar_thickness" android:layout_marginBottom="5dp"/>
        <TextView android:id="@+id/ch_5_text" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="8dp" android:text="Ch 5: 1500" style="@style/rc_textTitle"/>
        <org.droidplanner.android.widgets.FillBar.FillBar android:id="@+id/fillBar_ch_6" android:layout_width="@dimen/fillbar_length" android:layout_height="@dimen/fillbar_thickness" android:layout_marginBottom="5dp"/>
        <TextView android:id="@+id/ch_6_text" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="8dp" android:text="Ch 5: 1500" style="@style/rc_textTitle"/>
        <org.droidplanner.android.widgets.FillBar.FillBar android:id="@+id/fillBar_ch_7" android:layout_width="@dimen/fillbar_length" android:layout_height="@dimen/fillbar_thickness" android:layout_marginBottom="5dp"/>
        <TextView android:id="@+id/ch_7_text" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="8dp" android:text="Ch 5: 1500" style="@style/rc_textTitle"/>
        <org.droidplanner.android.widgets.FillBar.FillBar android:id="@+id/fillBar_ch_8" android:layout_width="@dimen/fillbar_length" android:layout_height="@dimen/fillbar_thickness" android:layout_marginBottom="5dp"/>
        <TextView android:id="@+id/ch_8_text" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="8dp" android:text="Ch 5: 1500" style="@style/rc_textTitle"/>
    </LinearLayout>
</LinearLayout>
